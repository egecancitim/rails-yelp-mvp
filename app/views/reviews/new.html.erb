<div class="container">
  <h1>Add a Review</h1>
  <%= simple_form_for [ @restaurant, @review ] do |f| %>
  <%= f.input :content %>
  <%= f.input :rating %>
  <%= f.submit "Add Review", class: "btn btn-primary" %>
  <% end %>
</div>
<div class="backlink">
 <%= link_to restaurant_path(@restaurant) do%>
  <i class="fas fa-arrow-circle-left"></i>
  <% end %>
</div>
